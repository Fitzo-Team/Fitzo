      "MealType": {
        "enum": [
          "Breakfast",
          "SecondBreakfast",
          "Lunch",
          "Dinner",
          "Snack",
          "Supper"
        ],
        "type": "string"
      },
      "ProductDto": {
        "type": "object",
        "properties": {
          "externalId": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "imageUrl": {
            "type": "string",
            "nullable": true
          },
          "calories": {
            "type": "number",
            "format": "double"
          },
          "protein": {
            "type": "number",
            "format": "double"
          },
          "fat": {
            "type": "number",
            "format": "double"
          },
          "carbs": {
            "type": "number",
            "format": "double"
