            "type": "string",
            "nullable": true
          },
          "nutriScore": {
            "type": "string",
            "nullable": true
          },
          "ecoScore": {
            "type": "string",
            "nullable": true
          },
          "hasPalmOil": {
            "type": "boolean",
            "nullable": true
          },
          "labels": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "allergens": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "dataQualityMessages": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "isDataComplete": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
