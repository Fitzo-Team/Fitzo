            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DietTag": {
        "enum": [
          "Vegan",
          "Vegetarian",
          "Keto",
          "HighProtein"
        ],
        "type": "string"
      },
      "FoodCategories": {
        "enum": [
          "Unknown",
          "Vegetables",
          "Fruits",
          "Dairy",
          "Meat",
          "Grains",
          "Fats",
          "Sweets",
          "Beverages",
          "NutsAndSeeds",
          "SpicesSauces"
        ],
        "type": "string"
      },
      "Gender": {
        "enum": [
          "Male",
          "Female"
        ],
        "type": "string"
      },
      "IngredientDto": {
        "required": [
          "product"
        ],
        "type": "object",
        "properties": {
          "product": {
            "$ref": "#/components/schemas/ProductDto"
          },
          "amount": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
